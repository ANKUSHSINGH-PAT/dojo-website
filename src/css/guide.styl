@import nib
@import variables
@import header
@import url('//fonts.googleapis.com/css?family=Lato:100,300,400,700|Merriweather:400,700,300,300italic,400italic,700italic');
@import 'vendor/foundation.css';
@import overrides;

body, html {
    margin:0; padding:0;
    width:100%;
    height:100%;
    font-family: Arial,Helvetica,"Liberation Sans",FreeSans,sans-serif;
}
body.wikidocs {
    font-size: 14px;
    font-family:Helvetica,Arial,Sans-Serif;
    line-height:1.3em;
}
.wikidocs code,
.wikidocs pre,
.wikidocs tt {
	font-family:"Consolas","Bitstream Vera Sans Mono","Courier New",Courier,monospace;
}
.wikidocs .wrap {
    width:960px;
    margin: 0 auto;
}
.wikidocs #contents {
    border:1px dashed #666;
    background: #eee;
    padding:0 15px 10px 15px;
    margin:15px 0;
}
.wikidocs #contents .simple {
    margin:5px;
    padding-left:15px;
    padding-right:5px;
}
.wikidocs .section h2,
.wikidocs .section h1 {
    border-bottom:1px solid #666;
    line-height:1.7em;
}
.wikidocs .section h1 a,
.wikidocs .section h2 a,
.wikidocs .section a.toc-backref {
    text-decoration:none;
}
.wikidocs .section h1 a:link,
.wikidocs .section h2 a:link,
.wikidocs .section a.toc-backref:link {
    color:#101010;
    text-decoration:none;
}
.wikidocs .section h1 a:visited,
.wikidocs .section h2 a:visited,
.wikidocs .section a.toc-backref:visited {
    color:#101010;
    text-decoration:none;
}
.wikidocs .section h1 a:hover,
.wikidocs .section h2 a:hover,
.wikidocs .section a.toc-backref:hover {
    color:#666;
    text-decoration:none;
}
.wikidocs #rstwiki-nav a { color:#000; }
.wikidocs #rstwiki-nav a:link { color:#000; text-decoration:underline; }
.wikidocs #rstwiki-nav a:visited { color:#101010; text-decoration:underline; }
.wikidocs #rstwiki-nav a:hover { color:red; text-decoration:underline; }
.wikidocs .head {
    background:#ededed;
    border-bottom:1px solid #666;
}
.wikidocs .crumbs {
    padding-top:10px;
    padding-bottom:10px;
}
.wikidocs .document {
    padding:5px 0 25px 0;
    position:relative;
}
.wikidocs .highlight .hll { background-color: #ffffcc }
.wikidocs .highlight,
.wikidocs .literal-block {
    border:1px dashed #666;
    padding:5px 15px;
    background: #f8f8f8;
}
.wikidocs .literal-block{
    padding: 1.5em 15px;
    overflow: auto;
    font-size: 94.2%;
}
.wikidocs .highlight pre { width:100%; overflow:auto; font-size:94.2%; }
